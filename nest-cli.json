{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/main-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/main-service",
  "projects": {
    "main-service": {
      "type": "application",
      "root": "apps/main-service",
      "entryFile": "main",
      "sourceRoot": "apps/main-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main-service/tsconfig.app.json"
      }
    },
    "invoices": {
      "type": "application",
      "root": "apps/invoices",
      "entryFile": "main",
      "sourceRoot": "apps/invoices/src",
      "compilerOptions": {
        "tsConfigPath": "apps/invoices/tsconfig.app.json"
      }
    },
    "app-log-service": {
      "type": "application",
      "root": "apps/app-log-service",
      "entryFile": "main",
      "sourceRoot": "apps/app-log-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-log-service/tsconfig.app.json"
      }
    },
    "app-users": {
      "type": "application",
      "root": "apps/app-users",
      "entryFile": "main",
      "sourceRoot": "apps/app-users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-users/tsconfig.app.json"
      }
    },
    "app-auth": {
      "type": "application",
      "root": "apps/app-auth",
      "entryFile": "main",
      "sourceRoot": "apps/app-auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-auth/tsconfig.app.json"
      }
    },
    "app-image": {
      "type": "application",
      "root": "apps/app-image",
      "entryFile": "main",
      "sourceRoot": "apps/app-image/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-image/tsconfig.app.json"
      }
    }
  }
}